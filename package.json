    "@types/lodash": "^4.14.177",
    "@types/node": "^16.11.10",
    "prettier": "^2.5.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.5.2"
  },
